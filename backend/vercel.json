{
    "version": 2,
    "name": "greenEarth",
    "builds": [
       { "src": "server.js", "use": "@vercel/node" }
    ],
    "routes": [
       { 
         "src": "/(.*)", 
         "dest": "/server.js",
         "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
         "headers": {
            "Access-Control-Allow-Origin": "*"
         }
         
      }
       
    ]
}
